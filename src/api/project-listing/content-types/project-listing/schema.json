      "required": true
    },
    "bannerImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "shortDescription": {
      "type": "string",
      "required": true
    },
    "keyFeatures": {
      "displayName": "keyFeatures",
      "type": "component",
      "repeatable": true,
      "component": "project.key-features"
    },
    "brochureFile": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "tags": {
      "type": "component",
      "repeatable": true,
      "component": "homepage.tags"
    },
    "timeline": {
      "displayName": "timeline",
      "type": "component",
      "repeatable": true,
      "component": "project.timeline"
    },
    "awards": {
      "displayName": "awards",
      "type": "component",
      "repeatable": true,
      "component": "project.awards"
    },
    "faq": {
      "type": "component",
