{
  "kind": "collectionType",
  "collectionName": "project_listings",
  "info": {
    "singularName": "project-listing",
    "pluralName": "project-listings",
    "displayName": "ProjectListing",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "projectName": {
      "type": "string",
      "required": true
    },
    "projectStatus": {
