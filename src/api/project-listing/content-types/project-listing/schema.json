    "amenityCategory": {
      "displayName": "amenityCategory",
      "type": "component",
      "repeatable": false,
      "component": "project.amenity-category"
    },
    "hero": {
      "displayName": "hero",
      "type": "component",
      "repeatable": false,
      "component": "project.hero"
    },
    "slug": {
      "type": "uid",
      "targetField": "projectName"
    },
    "developmentPlan": {
      "displayName": "DevelopmentPlan",
      "type": "component",
      "repeatable": false,
      "component": "project.development-plan"
    },
    "constructionProgress": {
      "displayName": "constructionProgress",
      "type": "component",
      "repeatable": false,
      "component": "project.construction-progress"
    },
    "hotspot": {
      "type": "component",
      "repeatable": false,
      "component": "project.hotspot"
    },
