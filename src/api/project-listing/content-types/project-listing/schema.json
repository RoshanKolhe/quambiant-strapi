    "calculator": {
      "displayName": "calculator",
      "type": "component",
      "repeatable": false,
      "component": "project.calculator"
    },
    "neighborhood": {
      "displayName": "neighborhood",
      "type": "component",
      "repeatable": false,
      "component": "project.neighborhood"
    },
    "projectGallery": {
      "displayName": "projectGallery",
      "type": "component",
      "repeatable": false,
      "component": "project.project-gallery"
    },
    "experience": {
      "displayName": "experience",
      "type": "component",
      "repeatable": false,
      "component": "project.experience"
    },
    "projectLayout": {
      "displayName": "projectLayout",
      "type": "component",
      "repeatable": false,
      "component": "project.project-layout"
